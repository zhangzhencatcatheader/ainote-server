export top.zztech.ainote.model.LedgerTemplate
    -> package top.zztech.ainote.service.dto

input CreateTemplate {
    name,
    description,
    category,
    id(icon) as iconFileId
    id(file) as fileId
    sortOrder
    color
}

input UpdateTemplate{
    id,
    fields {
        #allScalars(this)
    }
}



specification SearchTemplate {
    like/i(name,description) as keyword
    category
    enabled
}

input ChangeTemplateStatus {
    id
    enabled
}