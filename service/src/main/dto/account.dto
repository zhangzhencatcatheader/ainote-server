export top.zztech.ainote.model.Account
    -> package top.zztech.ainote.service.dto

input LoginInput {
    username,
    password
}

input  RegisterInput {
    username,
    phone,
    password
}

input UpdateInput {
    id?
    username,
    phone,
    id(avatar) as avatarId
}
specification AccountSearch {
    like/i(username,phone) as keyword
    status
    role
}

input JoinCompany {
    id?
    accountCompanies {
        id(company) as companyId
        role
    }
}