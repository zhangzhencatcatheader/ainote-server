# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ainote
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Aliyun OSS Configuration
# Note: endpoint must include https:// or http:// prefix
ALIYUN_OSS_ENDPOINT=https://oss-cn-shanghai.aliyuncs.com
ALIYUN_ACCESS_KEY_ID=your_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_access_key_secret_here
ALIYUN_OSS_BUCKET=your_bucket_name_here

# Aliyun PNVS SMS Configuration (Dypnsapi)
ALIYUN_PNVS_ACCESS_KEY_ID=your_access_key_id_here
ALIYUN_PNVS_ACCESS_KEY_SECRET=your_access_key_secret_here
ALIYUN_PNVS_SMS_REGION=cn-shanghai
ALIYUN_PNVS_SMS_ENDPOINT=dypnsapi.aliyuncs.com
ALIYUN_PNVS_SMS_SIGN_NAME=ainote
ALIYUN_PNVS_SMS_TEMPLATE_CODE=100001
ALIYUN_PNVS_SMS_CODE_LENGTH=4
ALIYUN_PNVS_SMS_VALID_TIME_SECONDS=300
ALIYUN_PNVS_SMS_MIN_SEND_INTERVAL_SECONDS=60

# Redis Configuration (Optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration
SERVER_PORT=8080

# Spring Profile (dev/prod)
SPRING_PROFILES_ACTIVE=dev
# 阿里通义千问(DashScope) API配置
DASHSCOPE_API_KEY=
DASHSCOPE_CHAT_MODEL=qwen-plus
## 百炼 OpenAI 兼容接口配置（用于 qwen-long 文件 fileid:// 能力）
## 如无需自定义可不填
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_LONG_MODEL=qwen-long